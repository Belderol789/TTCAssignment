# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'TTCAssignment' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TTCAssignment
pod 'R.swift' # R.Swift ensures no mistakes will be made when loading assets from misspelled Strings
pod 'SwiftLint' # SwiftLint ensures that all developers on a project follow the same set of rules and guidelines when writing code
pod 'Alamofire', '~> 5.2' # Alamofire is such a powerful networking library and it's very common almost every developer knows how to use it

end
